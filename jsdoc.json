{
  "source": {
    "include": ["./scripts/"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/jsdoc/templates/default",
    "readme": "./README.md"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}